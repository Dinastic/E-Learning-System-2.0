@model IEnumerable<E_Learning_System_2._0.Models.Student>

@{
    ViewData["Title"] = "Index";
}

<h1>Student index</h1>
<br />
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Surname)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>
                @TempData["name"]
            </td>
            <td>
                @TempData["surname"]
            </td>
        </tr>
    </tbody>
</table>
<br />
<div class="myButtons">
    <a class="buttonIndex" asp-action="ShowAllGrades" asp-route-id="@TempData["username"]"> Show all grades </a> |

    <a class="buttonIndex" asp-action="ShowSingleCourse" asp-route-id="@TempData["username"]"> Show grade for a specific course </a> |

    <a class="buttonIndex" asp-action="ShowSingleSemester" asp-route-id="@TempData["username"]"> Show grades for a specific semester </a>
</div>


